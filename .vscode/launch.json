{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug analytics-event-platform with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "analytics-event-platform"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/analytics-event-platform/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
